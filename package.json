{
  "name": "rentr",
  "version": "1.0.0",
  "description": "Rentr",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"npm run test\" \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "prebuild": "jest",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.7",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-session": "^1.17.2",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "client/"
    ]
  }
}
